{
  "name": "nuxt-oidc-auth",
  "type": "module",
  "version": "0.18.0",
  "private": false,
  "packageManager": "pnpm@9.11.0",
  "description": "OIDC authentication module for Nuxt SSR",
  "license": "MIT",
  "homepage": "https://github.com/itpropro/nuxt-oidc-auth#readme",
  "repository": "itpropro/nuxt-oidc-auth",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && pnpm client:build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "release": "pnpm lint && pnpm prepack && changelogen --release && git push --follow-tags && pnpm publish --access=public",
    "lint": "eslint . && tsc --noemit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.5.0",
    "@nuxt/devtools-ui-kit": "^1.5.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "h3": "^1.12.0",
    "jose": "^5.9.2",
    "ofetch": "^1.4.0",
    "scule": "^1.3.0",
    "sirv": "^2.0.4",
    "ufo": "^1.5.4",
    "uncrypto": "^0.1.3",
    "undio": "^0.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/kit": "^3.13.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "^22.5.5",
    "@unocss/eslint-plugin": "^0.62.4",
    "changelogen": "^0.5.7",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.0",
    "nuxt": "^3.13.2",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  },
  "pnpm": {
    "overrides": {
      "sharp": "0.33.5"
    }
  }
}
